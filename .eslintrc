{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "react-app",
    "react-app/jest"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/prop-types": "off",
    "class-methods-use-this": "off"
  }
}